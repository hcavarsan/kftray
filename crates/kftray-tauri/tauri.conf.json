    }
  },
  "app": {
    "withGlobalTauri": true,
    "macOSPrivateApi": true,
    "security": {
      "csp": null
    },
    "windows": [
      {
        "decorations": false,
        "focus": false,
        "fullscreen": false,
        "height": 500,
        "hiddenTitle": true,
        "resizable": false,
